################## end of tower P ##################




################## classifier network ##################
#### reshape & construct input
layer {
  bottom: "pool4"
  top: "classifier/reshape"
  name: "classifier/reshape"
  type: "Reshape"
  reshape_param {
    shape {
      dim: 0
      dim: -1
      dim: 1
      dim: 1
    }
  }
}
layer {
  bottom: "pool4_p"
  top: "classifier/reshape_p"
  name: "classifier/reshape_p"
  type: "Reshape"
  reshape_param {
    shape {
      dim: 0
      dim: -1
      dim: 1
      dim: 1
    }
  }
}
layer {
  bottom: "classifier/reshape"
  bottom: "classifier/reshape_p"
  top: "classifier/concat"
  name: "classifier/concat"
  type: "Concat"
  concat_param {
    axis: 1
  }
}
#### fc layers
layer {
  bottom: "classifier/concat"
  top: "fc1"
  name: "fc1"
  type: "InnerProduct"
  param {
    lr_mult : 1
  }
  param {
    lr_mult : 2
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "fc1_bn"
  type: "BatchNorm"
  bottom: "fc1"
  top: "fc1"
  batch_norm_param {
    use_global_stats: true
  }
}
layer {
  bottom: "fc1"
  top: "fc1"
  name: "fc1_relu"
  type: "ReLU"
}
layer {
  bottom: "fc1"
  top: "fc2"
  name: "fc2"
  type: "InnerProduct"
  param {
    lr_mult : 1
  }
  param {
    lr_mult : 2
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "fc2_bn"
  type: "BatchNorm"
  bottom: "fc2"
  top: "fc2"
  batch_norm_param {
    use_global_stats: true
  }
}
layer {
  bottom: "fc2"
  top: "fc2"
  name: "fc2_relu"
  type: "ReLU"
}
layer {
  bottom: "fc2"
  top: "prob"
  name: "fc3"
  type: "InnerProduct"
  param {
    lr_mult : 0
  }
  param {
    lr_mult : 0
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  bottom: "fc2"
  top: "center_pred"
  name: "fc3_p"
  type: "InnerProduct"
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "constant"
      value: 0
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

################## loss ##################
layer {
  name: "rescale_center"
  bottom: "center_target"
  top: "rescale_center"
  type: "Power"
  power_param {
    power: 1
    scale: 0.0078125 #0.00390625
    shift: -1
  }
}

layer {
  bottom : "prob"
  bottom : "confidence_label"
  top: "prob_loss"
  name : "prob_loss"
  type: "SoftmaxWithLoss"
  loss_weight: 0 #0.0001
}
layer {
  bottom: "rescale_center"
  bottom: "confidence_label"
  top: "positive_center_target"
  name: "positive_center_target"
  type: "Scale"
  scale_param {
    axis: 0
  }
}
layer {
  bottom: "center_pred"
  bottom: "confidence_label"
  top: "positive_center_pred"
  name: "positive_center_pred"
  type: "Scale"
  scale_param {
    axis: 0
  }
}
layer {
  name: "center_loss"
  type: "SmoothL1Loss"
  bottom: "positive_center_pred"
  bottom: "positive_center_target"
  top: "center_loss"
  loss_weight: 0.00000000001
}
layer {
  bottom: "positive_center_pred"
  top: "thres_loss"
  name: "thres_loss"
  type: "LocLoss"
  loc_loss_param {
    threshold: 1
  }
  loss_weight: 0.000001 #0.00000001
}